# Example environment file for jira-auto-worklog
# Copy this file to `.env` and fill in your actual credentials.

# Jira base URL (e.g. https://yourcompany.atlassian.net)
JIRA_URL=

# Default Jira issue/key to log hours to (e.g. PROJ-123)
JIRA_ISSUE=

# Jira session cookie (OBLIGATORIA para cuentas corporativas con SSO)
JIRA_COOKIE=JSESSIONID=<jsessionid>; tenant.session.token=<jwt_token>;

# Horas de trabajo de lunes a jueves 
HORAS_DIARIAS_NORMAL=8.5
# Horas de trabajo d√≠a viernes
HORAS_DIARIAS_REDUCIDA=6

# Comentario del worklog 
COMENTARIO=